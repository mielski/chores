# Example environment variables for local development
# Copy this file to .env and fill in your values

# Flask secret key - generate a secure random string
SECRET=your-very-secure-secret-key-here

# Login credentials for the application
APP_USERNAME=admin
APP_PASSWORD=your-secure-password-here
ACTION_PASSCODE=1234

# Azure resource group and container app name
AZ_RESOURCE_GROUP=your-resource-group-name
CONTAINER_APP_NAME=your-container-app-name

# cosmos db settings 
USE_COSMOS_DB=true
COSMOS_DB_ENDPOINT=
COSMOS_DB_KEY=